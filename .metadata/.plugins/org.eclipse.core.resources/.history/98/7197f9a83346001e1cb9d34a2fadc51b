package day10;

public class CardMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		CardPack cp=new CardPack();
		cp.shuffle();
//		for(int i=0; i<cp.getPack().length; i++) {
//			cp.getPack()[i].print();
//		}
		//4줄씩 13개로 찍기
		int index=0;
		for(int i=0; i<cp.getPack().length; i++) {
			cp.getPack()[i].print();
			index++;
			if(index%13==0) {
				System.out.println();
			}

		}

	}

}
